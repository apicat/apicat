{
  "private": true,
  "packageManager": "pnpm@6.32.2",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build:editor": "pnpm run -r --filter @ac/editor build",
    "build:doc-css": "pnpm run -r --filter @ac/doc-content-css build",
    "build:website": "pnpm run -r --filter @ac/website build",
    "build": "pnpm run -r build",
    "dev:editor": "pnpm run -r --filter @ac/editor dev",
    "dev:website": "pnpm run -r --filter @ac/website dev",
    "dev": "pnpm dev:website",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "clean:packages": "rm -rf ./packages/*/dist",
    "reset": "pnpm clean:packages && rm -rf ./node_modules && rm -rf ./packages/*/node_modules && pnpm install"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.38",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.1",
    "typescript": "~4.5.5",
    "vite": "^2.9.9",
    "vue-tsc": "^0.31.4"
  },
  "dependencies": {
    "vue": "^3.2.36"
  },
  "license": "MIT"
}